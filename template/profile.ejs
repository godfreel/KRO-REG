<% layout('layout/page') -%>
<% block('title', "User details") -%>
<form>
	<div class="container"> 
		
		<button class="btn btn-primary">Edit</button> 
		<button class="btn btn-success">Ok</button>
		<button class="btn btn-warning">Cancel</button>
	</div>
	<div class="container">
		<br>
		<table coumns="2" class="table" visible="true">
			<tr>
				<td><label class="col-lg-8">Name</label></td>
				<td  ><%=user.get('username')%></td>
			</tr>
			<tr>
				<td><label class="col-lg-8">Last Name</label></td>
				<td  ><%=user.get('lastname')%></td>
			</tr>
			<tr>
				<td><label class="col-lg-8">Birthday</label></td>
				<td  ><%=user.get('birthday')%></td>
			</tr>
			<tr>
				<td><label class="col-lg-8">Club</label></td>
				<td  ><%=user.get('club')%></td>
			</tr>
			<tr>
				<td><label class="col-lg-8">Role</label></td>
				<td  ><%=user.get('role')%></td>
			</tr>
			<tr>
				<td><label class="col-lg-8">Rank</label></td>
				<td ><%=user.get('rank')%></td>
			</tr>
			<tr>
				<td><label class="col-lg-8">Si-chip</label></td>
				<td  ><%=user.get('sichip')%></td>
			</tr>
		</table> 
  	
	<button class="btn btn-primary btn-lg" data-toggle="modal" data-target="#myModal">
  Launch demo modal
</button>
</div>
	<!-- Modal -->

	
</form>
<form>
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="myModalLabel">Modal title</h4>
      </div>
      <div class="modal-body">
        
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>
</form>

